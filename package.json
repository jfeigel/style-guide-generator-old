{
  "name": "style-guide-generator",
  "version": "1.0.0",
  "description": "Generate a SASS stylesheet based on user-defined settings",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.prod.config.js",
    "dev": "webpack-dev-server --port 5002 --host 0.0.0.0 --devtool eval --progress --colors --hot --content-base ./build --config ./webpack.config.js",
    "seed": "node ./helpers/seed.js",
    "start": "grunt",
    "production": "sudo NODE_ENV=production node index.js >> /var/log/style-guide-generator.log",
    "install": "bower install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfeigel/style-guide-generator.git"
  },
  "keywords": [
    "koa",
    "handlebars",
    "passport"
  ],
  "author": "James Feigel (@jfeigel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfeigel/style-guide-generator/issues"
  },
  "homepage": "https://github.com/jfeigel/style-guide-generator#readme",
  "dependencies": {
    "co": "^4.6.0",
    "es6-promise": "^3.1.2",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-generic-session": "^1.10.2",
    "koa-generic-session-rethinkdb": "^1.1.1",
    "koa-hbs": "^0.8.0",
    "koa-passport": "^1.3.1",
    "koa-router": "^5.3.0",
    "koa-static-folder": "^0.1.6",
    "lodash": "^4.8.2",
    "material-ui": "^0.15.0",
    "moment": "^2.13.0",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-slack": "0.0.6",
    "passport-twitter": "^1.0.4",
    "react": "^15.0.1",
    "react-bootstrap": "^0.28.5",
    "react-color": "^2.2.0",
    "react-dom": "^15.0.1",
    "react-router": "^2.2.4",
    "react-tap-event-plugin": "^1.0.0",
    "rethinkdb": "2.2.3",
    "rethinkdbdash": "^2.3.7"
